{
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "dependencies": {
    "get-value": "^2.0.6",
    "has-values": "^1.0.0",
    "isobject": "^3.0.0"
  },
  "description": "Returns true if a value exists, false if empty. Works with deeply nested values using object paths.",
  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/jonschlinkert/has-value",
  "license": "MIT",
  "main": "index.js",
  "name": "has-value",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/has-value.git"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "define-property",
        "get-value",
        "set-value",
        "unset-value"
      ]
    },
    "reflinks": [],
    "lint": {
      "reflinks": true
    }
  },
  "version": "1.0.0"
}