{
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "dependencies": {
    "is-absolute": "^1.0.0",
    "map-cache": "^0.2.0",
    "path-root": "^0.1.1"
  },
  "deprecated": false,
  "description": "Pollyfill for node.js `path.parse`, parses a filepath into an object.",
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.2.5",
    "should": "^7.0.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/jonschlinkert/parse-filepath",
  "license": "MIT",
  "main": "index.js",
  "name": "parse-filepath",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/parse-filepath.git"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "global-prefix",
      "list": [
        "global-prefix",
        "is-absolute",
        "is-relative",
        "relative"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "version": "1.0.2"
}